{
  "name": "midgard-txs-generator",
  "version": "0.1.0",
  "description": "Midgard Transaction Generator for L2",
  "engines": {
    "node": ">=18.18",
    "npm": ">=10.5"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "midgard-tx-generator": "./dist/bin/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "dev": "tsc --watch",
    "start": "node dist/bin/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint ./src --max-warnings=0",
    "format": "prettier -w '**/*.{js,jsx,ts,tsx,json,.*}'",
    "clean": "rm -rf ./dist ./generated-transactions"
  },
  "dependencies": {
    "@lucid-evolution/lucid": "^0.4.20",
    "effect": "^3.12.10",
    "p-limit": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "chalk": "^5.3.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  }
}
