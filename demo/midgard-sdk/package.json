    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.5.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "vitest": "2.1.8",
    "rimraf": "^6.0.1"
